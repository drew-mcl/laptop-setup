# Copy to .env and adjust. Used by bootstrap scripts.

# Git identity
GIT_USER_NAME=Your Name
GIT_USER_EMAIL=you@example.com

# Optional: GPG signing (set key id and toggle signing)
GIT_SIGNING_KEY=
GIT_COMMIT_SIGN=true

# Optional: corporate proxies
HTTP_PROXY=
HTTPS_PROXY=
NO_PROXY=localhost,127.0.0.1

# Brew / Java provider selection: oracle | temurin
JDK_PROVIDER=oracle

